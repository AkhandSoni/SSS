{
  "manifest_version": 3,
  "name": "SCube",
  "description": "Semantic Spoiler Shield â€“ aggressively blurs spoilers across the web.",
  "version": "1.1.10",
  "icons": {
    "128": "icon128.png"
  },
  "permissions": ["storage", "scripting"],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "css": ["blur.css"],
      "run_at": "document_idle"
    }
  ]
}